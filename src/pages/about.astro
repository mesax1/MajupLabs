---
import { getCollection } from "astro:content";
import PageLayout from "@/layouts/Base.astro";
import Note from "@/components/note/Note.astro";
import { collectionDateSort } from "@/utils/date";


const meta = {
	description: "MajupLabs delivers world-class AI engineering, consulting and development services. Expert solutions in Machine Learning, LLMs, RAG, and optimization for startups and established companies.",
	title: "About MajupLabs | Expert AI Consulting & Development",
};

// Notes
const MAX_NOTES = 2;
const allNotes = await getCollection("note");
const latestNotes = allNotes.sort(collectionDateSort).slice(0, MAX_NOTES);
